<h1>Tasks#index</h1>
<ul>
  <% @tasks.each do |task|%>
  <li>
    <h2><%= link_to task.title, task_path(task) %></h2>
    <p><%= task.desc %></p>
    <h3>
      <%= link_to 'edit', edit_task_path(task) %>
      <%= link_to 'delete', task_path(task), method: :delete %>
    </h3>
  </li>
  <% end %>
</ul>
